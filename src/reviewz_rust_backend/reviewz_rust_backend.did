type CreateUserPayload = record { username : text; role : text; email : text };
type User = record {
  username : text;
  role : text;
  user_id : nat64;
  email : text;
  joined_at : nat64;
};
service : {
  create_user : (CreateUserPayload) -> (opt User);
  view_all_user : () -> (opt vec User) query;
}
