type CreateUserPayload = record { username : text; role : text; email : text };
type Error = variant {
  NotFound : record { msg : text };
  InvalidUserData : record { msg : text };
};
type Result = variant { Ok : opt User; Err : Error };
type User = record {
  username : text;
  role : text;
  user_id : nat64;
  email : text;
  joined_at : nat64;
};
service : {
  create_user : (CreateUserPayload) -> (Result);
  view_all_user : () -> (opt vec User) query;
}
